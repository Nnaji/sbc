<footer id="footer-section" class="f-section">
  <div class="container">
    <div class="row">
      <div class="col-lg-4">
        <address id="c-address">
          <strong>{{companyName}} </strong><br>
          {{address}}<br>
           {{postCode}} {{city}} {{country}}<br>
           <abbr title="Phone" class="glyphicon glyphicon-earphone"> P:</abbr> {{phoneNumber}} <br>
           <abbr title="email" class="glyphicon glyphicon-envelope"> Email:</abbr> {{email}}
        </address>
        <div>
          <div class="col-lg-8">
              {{fullName}}
          </div>
    </div>
  </div>
</footer>
<script src="node_modules/jquery/dist/jquery.min.js"></script>
<script src="node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="dist/scripts/main.min.js"></script>
</body>
</html>
